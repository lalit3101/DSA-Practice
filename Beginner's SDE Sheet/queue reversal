// implemented using stacks
queue<int> rev(queue<int> q)
{
    // add code here.
    stack<int>s;
    int x;
    while(!q.empty()){
        x = q.front();
        q.pop();
        s.push(x);
    }
    while(!s.empty()) {
        int x = s.top();
        s.pop();
        q.push(x);
    }
    return q;
}
