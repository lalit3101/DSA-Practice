class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        unordered_map<int, int> visited;
        for (int i=0; i<nums.size(); i++) {
            int b = nums[i];
            int a = target - b;
            if (visited.count(a))
                return {visited[a], i};
            visited[b] = i;
        }
        return {};
        }
    };
